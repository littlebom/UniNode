{
  "name": "uni-node",
  "version": "0.1.0",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "test": "jest",
    "test:e2e": "jest --config test/jest-e2e.json",
    "test:integration": "jest --config test/jest-integration.json",
    "test:security": "jest --config test/jest-security.json --verbose",
    "db:migrate": "typeorm migration:run -d dist/data-source.js",
    "db:migrate:revert": "typeorm migration:revert -d dist/data-source.js",
    "db:seed": "ts-node src/database/seed.ts",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "load:jwt": "node ../../scripts/generate-test-jwt.js",
    "load:smoke": "k6 run test/load/smoke.js",
    "load:full": "k6 run test/load/full-load.js",
    "load:stress": "k6 run test/load/stress.js"
  },
  "dependencies": {
    "@digitalbazaar/vc-status-list": "^7.0.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@noble/ed25519": "^2.1.0",
    "@noble/hashes": "^1.4.0",
    "@unilink/crypto": "workspace:*",
    "@unilink/dto": "workspace:*",
    "@unilink/vc-core": "workspace:*",
    "axios": "^1.7.2",
    "bullmq": "^5.4.0",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.1",
    "cron": "^3.2.1",
    "firebase-admin": "^12.1.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "json-canonicalize": "^1.0.6",
    "node-vault": "^0.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.0",
    "@types/compression": "^1.8.1",
    "@types/cron": "^2.4.3",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.7",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
